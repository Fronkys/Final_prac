        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden" HorizontalAlignment="Center" Width="1024"  Grid.Row="1" Grid.ColumnSpan="3"/>
        <Button Content="Роли"  HorizontalAlignment="Center" VerticalAlignment="Center" Height="62" Width="335" Click="Button_Click" Grid.Column="2" Grid.Row="0"/>
        <Button Content="Сотрудники"  HorizontalAlignment="Center" VerticalAlignment="Center" Height="62" Width="335" Click="Button_Click_2" Grid.Row="0" Grid.Column="1" />
        <Button Content="Данные для входа" HorizontalAlignment="Center" VerticalAlignment="Center" Height="62" Width="335" Click="Button_Click_1"  Grid.Row="0" Grid.Column="0" />
    </Grid>
</Window>
